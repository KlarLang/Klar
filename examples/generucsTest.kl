@Use("java")
public void main(){
    double x = 1.0;
    double y = 6.0;
    double z = 4.0;
    
    double resposta = ruleOfThree(x, y, z);
    println("Levaram: " + resposta + " horas.");

    return null;
}

@Use("java")
public double ruleOfThree(double x, double y, double z){
    double resultado = z * y / x;

    return resultado;
}

